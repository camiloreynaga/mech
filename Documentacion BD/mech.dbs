	TP1.codPers,TP1.nombre+' '+TP1.apellido as nomPers,TPD.codPagD,TPD.nroP,TTP.codTipP,TTP.tipoP,TCJ.codCaj,TCJ.caja,TT.tipoMov+' - '+TCJ.caja as movimiento
